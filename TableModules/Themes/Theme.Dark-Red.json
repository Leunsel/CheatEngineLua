{
   "$schema":"vscode://schemas/color-theme",
   "name":"CustomTheme",
   "colors":{
      "editor.background":"#1C0B0B",
      "editor.foreground":"#F2DCDC",
      "editor.selectionHighlightBackground":"#7F1D1D",
      "editor.inactiveSelectionBackground":"#3B1818",
      "activityBarBadge.background":"#A93226"
   },
   "tokenColors":[
      {
         "name":"Comment",
         "scope":"comment",
         "settings":{
            "fontStyle":"",
            "foreground":"#8B4D4D"
         }
      },
      {
         "name":"String",
         "scope":"string",
         "settings":{
            "fontStyle":"",
            "foreground":"#D96B6B"
         }
      },
      {
         "name":"Keyword",
         "scope":"keyword",
         "settings":{
            "fontStyle":"",
            "foreground":"#E63946"
         }
      },
      {
         "name":"Variable",
         "scope":"variable",
         "settings":{
            "fontStyle":"",
            "foreground":"#F4A261"
         }
      },
      {
         "name":"VariableParameter",
         "scope":"variable.parameter",
         "settings":{
            "fontStyle":"",
            "foreground":"#E76F51"
         }
      },
      {
         "name":"KeywordControl",
         "scope":"keyword.control",
         "settings":{
            "fontStyle":"",
            "foreground":"#C0392B"
         }
      },
      {
         "name":"KeywordOperator",
         "scope":"keyword.operator",
         "settings":{
            "fontStyle":"",
            "foreground":"#E74C3C"
         }
      },
      {
         "name":"EntityNameFunction",
         "scope":"entity.name.function",
         "settings":{
            "fontStyle":"",
            "foreground":"#E74C3C"
         }
      },
      {
         "name":"EntityNameClass",
         "scope":"entity.name.class",
         "settings":{
            "fontStyle":"",
            "foreground":"#D1453B"
         }
      },
      {
         "name":"SupportClass",
         "scope":"support.class",
         "settings":{
            "fontStyle":"",
            "foreground":"#D32F2F"
         }
      },
      {
         "name":"ConstantNumeric",
         "scope":"constant.numeric",
         "settings":{
            "fontStyle":"",
            "foreground":"#D8345F"
         }
      }
   ],
   "semanticHighlighting":true
}
