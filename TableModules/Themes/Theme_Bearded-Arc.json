{
   "$schema":"vscode://schemas/color-theme",
   "name":"CustomTheme",
   "colors":{
      "editor.background":"#181421",
      "editor.foreground":"#c8c1d9",
      "editor.selectionHighlightBackground":"#9698d815",
      "editor.inactiveSelectionBackground":"#9698d84d",
      "activityBarBadge.background":"#9698d8"
   },
   "tokenColors":[
      {
         "name":"Comment",
         "scope":"comment",
         "settings":{
            "fontStyle":"",
            "foreground":"#c5bae159"
         }
      },
      {
         "name":"String",
         "scope":"string",
         "settings":{
            "fontStyle":"",
            "foreground":"#3CEC85"
         }
      },
      {
         "name":"Keyword",
         "scope":"keyword",
         "settings":{
            "fontStyle":"",
            "foreground":"#EACD61"
         }
      },
      {
         "name":"Variable",
         "scope":"variable",
         "settings":{
            "fontStyle":"",
            "foreground":"#FF738A"
         }
      },
      {
         "name":"VariableParameter",
         "scope":"variable.parameter",
         "settings":{
            "fontStyle":"",
            "foreground":"#F38CEC"
         }
      },
      {
         "name":"KeywordControl",
         "scope":"keyword.control",
         "settings":{
            "fontStyle":"",
            "foreground":"#EACD61"
         }
      },
      {
         "name":"KeywordOperator",
         "scope":"keyword.operator",
         "settings":{
            "fontStyle":"",
            "foreground":"#c8c1d960"
         }
      },
      {
         "name":"EntityNameFunction",
         "scope":"entity.name.function",
         "settings":{
            "fontStyle":"",
            "foreground":"#69C3FF"
         }
      },
      {
         "name":"EntityNameClass",
         "scope":"entity.name.class",
         "settings":{
            "fontStyle":"",
            "foreground":"#22ECDB"
         }
      },
      {
         "name":"SupportClass",
         "scope":"support.class",
         "settings":{
            "fontStyle":"",
            "foreground":"#B78AFF"
         }
      },
      {
         "name":"ConstantNumeric",
         "scope":"constant.numeric",
         "settings":{
            "fontStyle":"",
            "foreground":"#FF955C"
         }
      }
   ],
   "semanticHighlighting":true
}
