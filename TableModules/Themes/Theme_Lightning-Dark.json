{
   "colors":{
      "editor.background":"#0e0c29FF",
      "editor.foreground":"#76c6ffFF",
      "editor.selectionHighlightBackground":"#3E5AEF1A",
      "editor.inactiveSelectionBackground":"#3E5AEF1A",
      "activityBarBadge.background":"#65F7FFFF"
   },
   "tokenColors":[
      {
         "name":"Comment",
         "scope":[
            "comment",
            "punctuation.definition.comment"
         ],
         "settings":{
            "fontStyle":"italic",
            "foreground":"#4E548FFF"
         }
      },
      {
         "name":"String",
         "scope":[
            "string",
            "punctuation.definition.string"
         ],
         "settings":{
            "foreground":"#66ffdeFF"
         }
      },
      {
         "name":"Keyword,Storage",
         "scope":[
            "keyword",
            "storage.type",
            "storage.modifier"
         ],
         "settings":{
            "foreground":"#2681f9FF",
            "fontStyle":"bold"
         }
      },
      {
         "name":"Variables",
         "scope":[
            "variable",
            "stringconstant.other.placeholder"
         ],
         "settings":{
            "foreground":"#3EC5FFFF",
            "fontStyle":"italic"
         }
      },
      {
         "name":"Constant,FunctionArgument,TagAttribute,Embedded",
         "scope":[
            "constant.language",
            "support.constant",
            "constant.character",
            "constant.escape",
            "variable.parameter"
         ],
         "settings":{
            "foreground":"#3EC5FFFF"
         }
      },
      {
         "name":"Misc",
         "scope":[
            "punctuation",
            "meta.tag",
            "punctuation.definition.tag",
            "punctuation.separator.inheritance.php",
            "punctuation.definition.tag.html",
            "punctuation.definition.tag.begin.html",
            "punctuation.definition.tag.end.html",
            "punctuation.section.embedded",
            "keyword.other",
            "keyword.control",
            "keyword.operator.assignment"
         ],
         "settings":{
            "foreground":"#3c6affFF"
         }
      },
      {
         "name":"Number",
         "scope":[
            "constant.numeric"
         ],
         "settings":{
            "fontStyle":"bold",
            "foreground":"#ff7ca8FF"
         }
      },
      {
         "name":"Function,SpecialMethod",
         "scope":[
            "entity.name.function",
            "meta.function-call.generic",
            "variable.function",
            "keyword.other.special-method",
            "entity.name"
         ],
         "settings":{
            "foreground":"#21fbffFF"
         }
      },
      {
         "name":"CSSfunctions",
         "scope":[
            "support.function.misc.css",
            "support.function",
            "support.class"
         ],
         "settings":{
            "foreground":"#21fbffFF"
         }
      },
      {
         "name":"Invalid",
         "scope":[
            "invalid",
            "invalid.illegal"
         ],
         "settings":{
            "foreground":"#ff477eFF"
         }
      },
      {
         "name":"Colors",
         "scope":[
            "constant.other.color"
         ],
         "settings":{
            "fontStyle":"bold",
            "foreground":"#3EC5FFFF"
         }
      }
   ]
}
