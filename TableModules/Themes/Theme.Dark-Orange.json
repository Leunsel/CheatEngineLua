{
   "$schema":"vscode://schemas/color-theme",
   "name":"CustomTheme",
   "colors":{
      "editor.background":"#210900",
      "editor.foreground":"#FCECDD",
      "editor.selectionHighlightBackground":"#5A3B29",
      "editor.inactiveSelectionBackground":"#4E3224",
      "activityBarBadge.background":"#FFAA4C"
   },
   "tokenColors":[
      {
         "name":"Comment",
         "scope":"comment",
         "settings":{
            "fontStyle":"",
            "foreground":"#A86E46"
         }
      },
      {
         "name":"String",
         "scope":"string",
         "settings":{
            "fontStyle":"",
            "foreground":"#FFD27F"
         }
      },
      {
         "name":"Keyword",
         "scope":"keyword",
         "settings":{
            "fontStyle":"",
            "foreground":"#FFB347"
         }
      },
      {
         "name":"Variable",
         "scope":"variable",
         "settings":{
            "fontStyle":"",
            "foreground":"#F9B282"
         }
      },
      {
         "name":"VariableParameter",
         "scope":"variable.parameter",
         "settings":{
            "fontStyle":"",
            "foreground":"#E79A57"
         }
      },
      {
         "name":"KeywordControl",
         "scope":"keyword.control",
         "settings":{
            "fontStyle":"",
            "foreground":"#FF8743"
         }
      },
      {
         "name":"KeywordOperator",
         "scope":"keyword.operator",
         "settings":{
            "fontStyle":"",
            "foreground":"#FFB347"
         }
      },
      {
         "name":"EntityNameFunction",
         "scope":"entity.name.function",
         "settings":{
            "fontStyle":"",
            "foreground":"#FFAA4C"
         }
      },
      {
         "name":"EntityNameClass",
         "scope":"entity.name.class",
         "settings":{
            "fontStyle":"",
            "foreground":"#D98242"
         }
      },
      {
         "name":"SupportClass",
         "scope":"support.class",
         "settings":{
            "fontStyle":"",
            "foreground":"#FFB86C"
         }
      },
      {
         "name":"ConstantNumeric",
         "scope":"constant.numeric",
         "settings":{
            "fontStyle":"",
            "foreground":"#FF9F6E"
         }
      }
   ],
   "semanticHighlighting":true
}
