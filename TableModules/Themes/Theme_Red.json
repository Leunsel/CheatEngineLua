{
   "$schema":"vscode://schemas/color-theme",
   "name":"Red",
   "colors":{
      "editor.background":"#390000",
      "editor.foreground":"#F8F8F8",
      "editor.selectionHighlightBackground":"#f5500039"
   },
   "tokenColors":[
      {
         "name":"Comment",
         "scope":"comment",
         "settings":{
            "fontStyle":"italic",
            "foreground":"#e7c0c0ff"
         }
      },
      {
         "name":"Keyword",
         "scope":"keyword",
         "settings":{
            "fontStyle":"",
            "foreground":"#f12727ff"
         }
      },
      {
         "name":"String",
         "scope":"string",
         "settings":{
            "fontStyle":"",
            "foreground":"#cd8d8dff"
         }
      },
      {
         "name":"Variable",
         "scope":"variable",
         "settings":{
            "fontStyle":"italic",
            "foreground":"#fb9a4bff"
         }
      },
      {
         "name":"Invalid",
         "scope":"invalid",
         "settings":{
            "foreground":"#ffffffff"
         }
      }
   ],
   "semanticHighlighting":true
}
