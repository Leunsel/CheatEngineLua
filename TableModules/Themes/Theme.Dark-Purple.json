{
   "$schema":"vscode://schemas/color-theme",
   "name":"CustomTheme",
   "colors":{
      "editor.background":"#15131c",
      "editor.foreground":"#E6E1FF",
      "editor.selectionHighlightBackground":"#3A3250",
      "editor.inactiveSelectionBackground":"#2E2840",
      "activityBarBadge.background":"#B794F4"
   },
   "tokenColors":[
      {
         "name":"Comment",
         "scope":"comment",
         "settings":{
            "fontStyle":"",
            "foreground":"#9A7FAE"
         }
      },
      {
         "name":"String",
         "scope":"string",
         "settings":{
            "fontStyle":"",
            "foreground":"#D4BFFF"
         }
      },
      {
         "name":"Keyword",
         "scope":"keyword",
         "settings":{
            "fontStyle":"",
            "foreground":"#B794F4"
         }
      },
      {
         "name":"Variable",
         "scope":"variable",
         "settings":{
            "fontStyle":"",
            "foreground":"#E1D9FF"
         }
      },
      {
         "name":"VariableParameter",
         "scope":"variable.parameter",
         "settings":{
            "fontStyle":"",
            "foreground":"#CBA6F7"
         }
      },
      {
         "name":"KeywordControl",
         "scope":"keyword.control",
         "settings":{
            "fontStyle":"",
            "foreground":"#A084CA"
         }
      },
      {
         "name":"KeywordOperator",
         "scope":"keyword.operator",
         "settings":{
            "fontStyle":"",
            "foreground":"#BCA0F7"
         }
      },
      {
         "name":"EntityNameFunction",
         "scope":"entity.name.function",
         "settings":{
            "fontStyle":"",
            "foreground":"#D8B6FF"
         }
      },
      {
         "name":"EntityNameClass",
         "scope":"entity.name.class",
         "settings":{
            "fontStyle":"",
            "foreground":"#A684D6"
         }
      },
      {
         "name":"SupportClass",
         "scope":"support.class",
         "settings":{
            "fontStyle":"",
            "foreground":"#C0A4E8"
         }
      },
      {
         "name":"ConstantNumeric",
         "scope":"constant.numeric",
         "settings":{
            "fontStyle":"",
            "foreground":"#8E72BF"
         }
      }
   ],
   "semanticHighlighting":true
}
