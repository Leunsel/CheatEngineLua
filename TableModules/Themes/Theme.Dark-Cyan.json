{
   "$schema":"vscode://schemas/color-theme",
   "name":"CustomTheme",
   "colors":{
      "editor.background":"#0A1A1A",
      "editor.foreground":"#A1C6C6",
      "editor.selectionHighlightBackground":"#006666",
      "editor.inactiveSelectionBackground":"#003333",
      "activityBarBadge.background":"#009999"
   },
   "tokenColors":[
      {
         "name":"Comment",
         "scope":"comment",
         "settings":{
            "fontStyle":"",
            "foreground":"#66B2B2"
         }
      },
      {
         "name":"String",
         "scope":"string",
         "settings":{
            "fontStyle":"",
            "foreground":"#4DB8B8"
         }
      },
      {
         "name":"Keyword",
         "scope":"keyword",
         "settings":{
            "fontStyle":"",
            "foreground":"#00B3B3"
         }
      },
      {
         "name":"Variable",
         "scope":"variable",
         "settings":{
            "fontStyle":"",
            "foreground":"#66CCCC"
         }
      },
      {
         "name":"VariableParameter",
         "scope":"variable.parameter",
         "settings":{
            "fontStyle":"",
            "foreground":"#99CCCC"
         }
      },
      {
         "name":"KeywordControl",
         "scope":"keyword.control",
         "settings":{
            "fontStyle":"",
            "foreground":"#009999"
         }
      },
      {
         "name":"KeywordOperator",
         "scope":"keyword.operator",
         "settings":{
            "fontStyle":"",
            "foreground":"#00B3B3"
         }
      },
      {
         "name":"EntityNameFunction",
         "scope":"entity.name.function",
         "settings":{
            "fontStyle":"",
            "foreground":"#00CCCC"
         }
      },
      {
         "name":"EntityNameClass",
         "scope":"entity.name.class",
         "settings":{
            "fontStyle":"",
            "foreground":"#00B3B3"
         }
      },
      {
         "name":"SupportClass",
         "scope":"support.class",
         "settings":{
            "fontStyle":"",
            "foreground":"#33CCCC"
         }
      },
      {
         "name":"ConstantNumeric",
         "scope":"constant.numeric",
         "settings":{
            "fontStyle":"",
            "foreground":"#33B3B3"
         }
      }
   ],
   "semanticHighlighting":true
}
